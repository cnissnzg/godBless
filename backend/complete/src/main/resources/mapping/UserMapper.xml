<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.watermark.mapper.UserMapper">

    <select id="getUser" resultType="com.watermark.entity.User">
        SELECT * FROM Watermark.User WHERE `uid` = #{uid};
    </select>

    <insert id="addUser" parameterType="com.watermark.entity.User">
        INSERT INTO  Watermark.User (`uid`, `password`,`email`,`role`) VALUES (#{username},#{password},#{email},#{role});
    </insert>


</mapper>