<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.watermark.mapper.ChallengeMapper">
    <select id="getProblemChallenge" resultType="com.watermark.entity.Challenge">
        SELECT * FROM Watermark.ProblemChallenge WHERE `pid`=#{pid};
