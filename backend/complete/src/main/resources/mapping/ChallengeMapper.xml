<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.watermark.mapper.ChallengeMapper">
    <select id="getProblemChallenge" resultType="com.watermark.entity.Challenge">
        SELECT `pid`,`cid`,`params` FROM Watermark.ProblemChallenge WHERE `pid`=#{pid};
    </select>
    <select id="getChallenge" resultType="com.watermark.entity.ChallengeParam">
        SELECT * FROM Watermark.ChallengeParam;
    </select>
    <insert id="add" parameterType="com.watermark.entity.Material">
        INSERT INTO  Watermark.ProblemChallenge (`pid`, `cid`,`params`) VALUES (#{pid},#{cid},#{params});
    </insert>
</mapper>